{{#with navigation}}
<div class="card mb-4">

    <div class="row">
        <div class="col-8">
            <h1 class="ms-3">{{Logo}}</h1>
        </div>

        <div class="col-4">
            <div class="nav mt-2">
                {{#each links}}
                <div>
                    <a class="nav-link {{#if @last}} disabled {{/if}}" href="{{this.href}}">{{this.text}}</a>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
</div>
{{/with}}